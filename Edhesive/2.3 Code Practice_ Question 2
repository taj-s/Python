num1 = input("Enter the Feet: ")
num2 = input("Enter the Inches: ")
num3 = input("Enter the Feet: ")
num4 = input("Enter the Inches: ")
ft1 = (num1)
ft2 = (num3)
in1 = (num2)
in2 = (num4)
sum1 = int(ft1) + int(ft2)
sum2 = int(sum1) * int(12)
sum3 = int(in1) + int(in2) + int(sum2)
sum4 = int(sum3) / int(12)
sum5 = int(sum4)
sum6 = int(sum3) % int(12)
print (str("Feet: ") + str(sum5) + str(" Inches: ") + str(sum6))
